package com;

public class Employee {
	private int id;
	private String name;
	private float salary;
	
	@Override
	public String toString() {
		return "Employee [id=" + id + ", name=" + name + ", salary=" + salary + "]";
	}

	Employee(){
		System.out.println("object created");
	}

	public void display() {
		System.out.println("this is employee class method");
	}
}
